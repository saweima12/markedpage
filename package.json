{
  "version": "0.0.19.1",
  "name": "markedpage",
  "description": "Markdown source provider & classification utilities",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "rollup -c",
    "test": "uvu -r tsm test test.ts$",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "files": [
    "dist/*"
  ],
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "types": "./dist/index.mjs.d.ts"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-sucrase": "^4.0.2",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/marked": "^4.0.3",
    "@types/remove-markdown": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "changeset": "^0.2.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.4.1",
    "rollup": "^2.64.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.22",
    "tslib": "^2.3.1",
    "tsm": "^2.2.1",
    "typescript": "^4.4.3",
    "uvu": "^0.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saweima12/markedpage"
  },
  "type": "module",
  "license": "MIT",
  "author": "Saweima <saweima12@gmail.com>",
  "keywords": [
    "markdown",
    "blog",
    "marked",
    "svelte"
  ],
  "dependencies": {
    "front-matter": "^4.0.2",
    "marked": "^4.0.14",
    "remove-markdown": "^0.5.0"
  }
}
